{
    "description": "Detokenizes tokenized text. If the language is unsupported, enter 'other'. Uses sacremoses",
    "type": "monolingual",
    "command": "sacremoses detokenize -l $LANG1",
    "parameters": {
        "LANG1":
        {
            "type": "str",
            "default": "en",
            "allowed_values":
