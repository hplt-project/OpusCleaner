                "ca",
                "cs",
                "cjk",
                "da",
                "de",
                "en",
                "el",
                "es",
                "et",
                "eu",
                "fi",
                "fr",
                "ga",
                "gl",
                "hi",
                "hr",
                "hu",
                "is",
                "it",
                "ko",
                "lt",
                "lv",
                "mt",
                "nb",
                "nl",
                "no",
                "nn",
                "pl",
                "pt",
                "ro",
                "ru",
                "sk",
                "sl",
                "sv",
                "zh",
                "other"
            ]
        }
    }
}
